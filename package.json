    "packages/rtf-readme",
    "packages/vscode-rtf-readme"
  ],
  "devDependencies": {
    "@mufan/code": "^0.2.10",
    "@mufan/eslint-plugin": "^0.1.48",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.18",
    "eslint": "^8.10.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
